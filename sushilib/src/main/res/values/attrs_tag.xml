<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SushiTag">
        <attr name="tagType" format="enum">
            <!-- Values should be same as com.zomato.sushilib.annotations.TagType  -->
            <enum name="rounded" value="0" />
            <enum name="capsule" value="1" />
            <enum name="rounded_outline" value="2" />
            <enum name="capsule_outline" value="3" />
        </attr>
        <attr name="tagSize" format="enum">
            <!-- Values should be same as com.zomato.sushilib.annotations.TagSize  -->
            <enum name="large" value="0" />
            <enum name="medium" value="1" />
            <enum name="small" value="2" />
            <enum name="tiny" value="3" />
        </attr>
        <attr name="tagColor" format="color" />
        <attr name="android:textColor" />
        <attr name="drawableLeft" />
        <attr name="drawableRight" />
        <attr name="drawableStart" />
        <attr name="drawableEnd" />
        <attr name="drawablePadding" />


    </declare-styleable>
</resources>